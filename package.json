{
  "name": "@moirei/vuetify-dynamic-form",
  "version": "2.0.0",
  "description": "Dynamically create Vuetify 3 forms with Vue 3. Works with and without Nuxt.",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./nuxt": "./nuxt/module.ts"
  },
  "files": [
    "dist",
    "runtime",
    "nuxt",
    "src"
  ],
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "vuetify3",
    "form",
    "dynamic",
    "dynamic-form",
    "vee-validate",
    "validation",
    "nuxt",
    "nuxt3",
    "typescript"
  ],
  "author": "Augustus Okoye <augustusokoye@moirei.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moirei/vuetify-dynamic-form.git"
  },
  "bugs": {
    "url": "https://github.com/moirei/vuetify-dynamic-form/issues"
  },
  "homepage": "https://github.com/moirei/vuetify-dynamic-form#readme",
  "dependencies": {
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.8.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "nuxt": "^3.8.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  },
  "peerDependencies": {
    "vee-validate": "^4.0.0",
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.0.2+sha512.4e502bea682e7d8004561f916f1da2dfbe6f718024f6aa50bf8cd86f38ea3a94a7f1bf854a9ca666dd8eafcfb8d44baaa91bf5c7876e79a7aeac952c332f0e88"
}
